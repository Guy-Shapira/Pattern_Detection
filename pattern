select *
from Event#length(10) as a1,
Event#length(10) as a2,
Event#length(10) as a3,
Event#length(10) as a4,
Event#length(10) as a5,
Event#length(10) as a6
where
a1.type = 'A' and
a2.type = 'A' and
a3.type = 'A' and
a4.type = 'A' and
a5.type = 'A' and
a6.type = 'A' and
a1.count < a2.count and 
a2.count < a3.count and 
a3.count < a4.count and 
a4.count < a5.count and 
a5.count < a6.count 
done